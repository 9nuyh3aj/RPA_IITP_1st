          <ui:InvokeCode ContinueOnError="{x:Null}" sap2010:Annotation.AnnotationText="단시간 근로자는 인원수를 따로 계산" Code="try{&#xD;&#xA;&#x9;// 단시간 근로자(고정)&#xD;&#xA;&#x9;string[] strArr_partTimers = str_partTimers.Split(',');&#xD;&#xA;&#x9;// 단시간 근로자 사번 목록을 추출&#xD;&#xA;&#x9;var partTimeEmpIds = dt_ptWorker.AsEnumerable()&#xD;&#xA;&#x9;&#x9;.Select(row =&gt; row.Field&lt;string&gt;(&quot;직원번호&quot;))&#xD;&#xA;&#x9;&#x9;.ToHashSet();&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;// 인사정보 DataTable에서 인원수 칼럼을 채움&#xD;&#xA;&#x9;foreach(DataRow row in io_dt_data.Rows){&#xD;&#xA;&#x9;&#x9;string empId = row.Field&lt;string&gt;(&quot;직원번호&quot;);&#xD;&#xA;&#x9;&#x9;if(partTimeEmpIds.Contains(empId)){&#xD;&#xA;&#x9;&#x9;&#x9;// 단시간 근로자 데이터에서 해당 사번의 행을 찾음&#xD;&#xA;&#x9;&#x9;&#x9;DataRow partTimeRow = dt_ptWorker.AsEnumerable()&#xD;&#xA;&#x9;&#x9;&#x9;.FirstOrDefault(r =&gt; r.Field&lt;string&gt;(&quot;직원번호&quot;) == empId);&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;if(partTimeRow != null){&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;// 월(근무시간) ~ 금(근무시간) 의 값을 가져와 8로 나눈 평균 계산&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;double totalMinutes = 0;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;int count = 0;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;string[] days = {&quot;월&quot;, &quot;화&quot;, &quot;수&quot;, &quot;목&quot;, &quot;금&quot;};&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;foreach(string day in days){&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;string columnName = $&quot;{day}(근무시간)&quot;;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;string timeStr = partTimeRow.Field&lt;string&gt;(columnName);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;if(!string.IsNullOrEmpty(timeStr)){&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;//HHmm 형식의 문자열을 분 단위로 변환&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;int hours = int.Parse(timeStr.Substring(0, 2));&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;int minutes = int.Parse(timeStr.Substring(2,2));&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;totalMinutes += (hours * 60) + minutes;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;count++;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;// 8시간으로 나눈 평균값 계산&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;double average = (totalMinutes / count) / 480.0;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;row[&quot;인원수&quot;] = average.ToString();&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;else if(strArr_partTimers.Contains(empId)){&#xD;&#xA;&#x9;&#x9;&#x9;// 단시간 근로제(고정) -&gt; 0.5&#xD;&#xA;&#x9;&#x9;&#x9;row[&quot;인원수&quot;] = &quot;0.5&quot;;&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;else{&#xD;&#xA;&#x9;&#x9;&#x9;row[&quot;인원수&quot;] = &quot;1&quot;;&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;// 결과 출력&#xD;&#xA;&#x9;foreach(DataRow dr in io_dt_data.Rows){&#xD;&#xA;&#x9;&#x9;&#x9;string name = dr[&quot;성명&quot;].ToString();&#xD;&#xA;&#x9;&#x9;&#x9;string count = dr[&quot;인원수&quot;].ToString();&#xD;&#xA;&#x9;&#x9;&#x9;if(count != &quot;1&quot;){&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;Console.WriteLine($&quot;{name} : {count}&quot;);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#xD;&#xA;}&#xD;&#xA;catch(Exception e){&#xD;&#xA;&#x9;Console.WriteLine(e.Message);&#xD;&#xA;}" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="586,196" sap2010:WorkflowViewState.IdRef="InvokeCode_2" Language="CSharp">
